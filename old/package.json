{
    "name": "bleachcss",
    "version": "0.0.1",
    "scripts": {
        "watch": "tsc -w -p ./server",
        "debug": "nodemon --watch ./dist --inspect=0.0.0.0:5858 --nolazy ./dist/server/index.js",
        "start": "node ./dist/server/index.js",
        "test": "node node_modules/.bin/jest",
        "test:watch": "node node_modules/.bin/jest --watch",
        "test:coverage": "node node_modules/.bin/jest --coverage",
        "test:coverage:ci": "node node_modules/.bin/jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
    },
    "devDependencies": {
        "@types/core-js": "2.5.0",
        "@types/cors": "2.8.4",
        "@types/jest": "23.1.4",
        "@types/node": "10.5.2",
        "@types/winston": "2.3.9",
        "coveralls": "3.0.2",
        "jest": "23.3.0",
        "nodemon": "1.11.0",
        "prettier": "2.2.1",
        "ts-jest": "23.0.0",
        "typescript": "2.3.2"
    },
    "main": "./dist/index.js",
    "author": "Thomas Genin <thomas@bleachcss.com>",
    "repository": "https://github.com/genintho/bleachcss.git",
    "license": "MIT",
    "description": "In browser detection of unused CSS. Used selector are send back to an API endpoint.",
    "keywords": [
        "unused",
        "unused css",
        "selector",
        "clean up",
        "CSS"
    ],
    "jest": {
        "projects": [
            "./probe/jest.config.js",
            "./server/jest.config.js"
        ]
    },
    "prettier": {
        "trailingComma": "es5",
        "arrowParens": "always",
        "useTabs": true
    },
    "dependencies": {
        "body-parser": "1.18.2",
        "cors": "2.8.4",
        "express": "4.16.3",
        "fs-extra": "^6.0.1",
        "kue": "^0.11.6",
        "lodash": "4.17.10",
        "moment": "2.22.2",
        "postcss": "6.0.23",
        "postcss-nested": "3.0.0",
        "postcss-safe-parser": "3.0.1",
        "sqlite3": "4.0.1",
        "targz": "1.0.1"
    }
}
